[user]
	name = Ryan Petrello
	email = lists@ryanpetrello.com
[core]
	excludesfile = /Users/ryan/.gitexcludes
[alias]
    ci = commit
    st = status
    info = remote show origin
[color]
	diff = always
	status = always
	branch = always
[color "status"]
    added = green
    changed = yellow
    untracked = cyan
[diff]
    ignoreSubmodules = untracked
